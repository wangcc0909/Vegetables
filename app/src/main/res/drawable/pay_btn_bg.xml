<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorEB161E"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="0"
                android:endColor="@color/colorF95A23"
                android:startColor="@color/F12E12"/>
        </shape>
    </item>
</selector>